{
    order rate_limit before basicauth
}

www.wordnet.dk {
    redir https://wordnet.dk{uri}
}

# TODO: eventually remove entirely
beta.wordnet.dk {
    redir https://wordnet.dk/dannet/page/welcome
}

wordnet.dk, localhost {
    # TODO: eventually remove redirect
    redir / https://cst.ku.dk/projekter/dannet

    rate_limit {remote.ip} 120r/m
    encode gzip
    reverse_proxy dannet:3456
}
